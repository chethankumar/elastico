{
  "name": "elastico",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:macos": "tauri build --target universal-apple-darwin",
    "build:macos-arm64": "tauri build --target aarch64-apple-darwin",
    "build:macos-x64": "tauri build --target x86_64-apple-darwin",
    "build:all": "npm run build:macos",
    "build:dev": "tauri build --debug",
    "dev:unsigned": "tauri dev --no-watch"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "autoprefixer": "^10.4.18",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "lucide-react": "^0.484.0",
    "monaco-editor": "^0.47.0",
    "nanoid": "^5.1.5",
    "postcss": "^8.4.37",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-monaco-editor": "^0.55.0",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "^3.4.3",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.8.2",
    "vite": "^6.0.3"
  }
}
